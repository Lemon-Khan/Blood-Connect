<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
<item android:state_pressed="true">
     <shape>
            <solid
                android:color="#131013" />
            <stroke
                android:width="1dp"
                android:color="#164F87" />
            <corners
                android:radius="20dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
</item>  
    
<item>
  <shape android:shape="oval" >
        <stroke
            android:height="1.0dip"
            android:width="1.0dip"
            android:color="#9DAA7C" />

        <solid android:color="#000000" />

        <corners
            android:bottomLeftRadius="14.0dip"
            android:bottomRightRadius="14.0dip"
            android:radius="14.0dip"
            android:topLeftRadius="14.0dip"
            android:topRightRadius="14.0dip" />
    </shape>
    </item>

</selector>
